<Project>
  <PropertyGroup>
    <LangVersion Condition="'$(LangVersion)' == ''">latest</LangVersion>
    <TargetFrameworks Condition="'$(TargetFrameworks)' == '' And '$(TargetFramework)' == ''">net6.0</TargetFrameworks>
